{
  "name": "streak-trivia",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build && npm run preview",
    "dev:watch": "chokidar \"src/**/*\" \"public/**/*\" -c \"npm run build\"",
    "dev:serve": "npx serve out -l 3000",
    "dev:static": "concurrently -k -n build,serve \"npm:dev:watch\" \"npm:dev:serve\"",
    "build": "next build && node scripts/relativize-html.js",
    "start": "npx serve out -l 3000",
    "test": "jest --passWithNoTests",
    "export": "next build && node scripts/relativize-html.js",
    "preview": "npx serve out -l 3000",
    "deploy": "gh-pages -d out"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.5",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "gh-pages": "^6.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
